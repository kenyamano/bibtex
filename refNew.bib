@Article{cheibub_rights_2020,
  Author = {Cheibub, Jose Antonio and Hong, Ji Yeon Jean and Przeworski, Adam},
  Institution = {Social Science Research Network},
  Journal = {SSRN} Scholarly Paper},
  Number = {ID} 3645410},
  Title = {Rights and Deaths: Government Reactions to the Pandemic},
